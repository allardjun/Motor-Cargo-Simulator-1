Cell["\<\
Ambarish takes his run lengths and velocities from Schnizter 2000.  For \
velocity, we have\
\>", "Text",
 CellChangeTimes->{{3.735681678328908*^9, 3.7356817350480003`*^9}, {
  3.7356836887535686`*^9, 
  3.7356837426289253`*^9}},ExpressionUUID->"523b09f9-9840-48af-9e78-\
51929f3fa9b9"],

Cell[BoxData[
 RowBox[{
  RowBox[{"v", "[", "f_", "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"kcat", " ", "d", " ", 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"f", "/", "f0"}], ")"}], "^", "2"}]}], ")"}]}], ")"}], " ", 
   RowBox[{"c", "/", 
    RowBox[{"(", 
     RowBox[{"c", "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"kcat", "+", 
         RowBox[{"koff0", " ", 
          RowBox[{"Exp", "[", 
           RowBox[{"f", " ", 
            RowBox[{"dl", " ", "/", 
             RowBox[{"(", 
              RowBox[{"kB", " ", "T"}], ")"}]}]}], "]"}]}]}], ")"}], "/", 
       "kon"}]}], ")"}]}]}]}]], "Input",
 CellChangeTimes->{{3.735683801137238*^9, 3.735683985763411*^9}, {
  3.735684081063533*^9, 3.735684107983041*^9}},
 CellLabel->"In[55]:=",ExpressionUUID->"87df9451-d0dc-41c0-94cd-1b0d3a92ebd8"],

Cell["The fit for run length is given as", "Text",
 CellChangeTimes->{{3.735683716811427*^9, 
  3.7356837306925383`*^9}},ExpressionUUID->"927914fa-4819-4143-b397-\
8b1bf02ba039"],

Cell[BoxData[
 RowBox[{
  RowBox[{"L", "[", "f_", "]"}], ":=", 
  RowBox[{"d", " ", "c", " ", "a", " ", 
   RowBox[{
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "f"}], " ", 
      RowBox[{"\[Delta]l", "/", 
       RowBox[{"(", 
        RowBox[{"kB", " ", "T"}], ")"}]}]}], "]"}], "/", 
    RowBox[{"(", 
     RowBox[{"c", "+", 
      RowBox[{"b", 
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"a", " ", 
          RowBox[{"Exp", "[", 
           RowBox[{
            RowBox[{"-", "f"}], " ", 
            RowBox[{"\[Delta]l", "/", 
             RowBox[{"(", 
              RowBox[{"kB", " ", "T"}], ")"}]}]}], "]"}]}]}], ")"}]}]}], 
     ")"}]}]}]}]], "Input",
