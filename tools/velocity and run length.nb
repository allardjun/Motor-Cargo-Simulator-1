 RowBox[{
  RowBox[{"koff", "[", "f_", "]"}], ":=", 
  RowBox[{"Piecewise", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"\[Epsilon]0", " ", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"Abs", "[", "f", "]"}], "/", "fd"}], "]"}]}], ",", 
       RowBox[{"f", "\[LessEqual]", "fs"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"a", "+", 
        RowBox[{"b", " ", "f"}]}], ",", 
       RowBox[{"f", ">", "fs"}]}], "}"}]}], "}"}], "]"}]}]], "Input",
