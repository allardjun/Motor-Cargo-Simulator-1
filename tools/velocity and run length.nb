Cell[BoxData[
 RowBox[{
  RowBox[{"kstep", "[", "f_", "]"}], ":=", 
  RowBox[{"Piecewise", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"v", "/", "dstep"}], " ", 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"f", "/", "fs"}], ")"}], "^", "w"}]}], ")"}]}], ",", 
       RowBox[{"0", "<", "f", "<", "fs"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"0", ",", 
       RowBox[{"f", "\[GreaterEqual]", "fs"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"v", "/", "dstep"}], ",", 
       RowBox[{"f", "<", "0"}]}], "}"}]}], "}"}], "]"}]}]], "Input",
